    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(axios)/)"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
